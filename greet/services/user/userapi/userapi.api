syntax = "v1"

type (
    SignupRequest {
        name string         `json:"name"`
        email string        `json:"email"`
        password string     `json:"password"`
    }
    SignupResponse {
        id int64           `json:"id"`
    }
)

service userapi-api {
	@handler UserapiHandler
	get /from/:name (Request) returns (Response)
}

